{% extends 'layouts/main.twig' %}


{% block content %}
    
    <div class="container">
        {% for row in articles|batch(3) %}
            <div class="row">
                {% for article in row %}
                    <div class="four columns">
                        {% include 'blocks/rollup.twig' %}
                    </div>
                {% endfor %}
            </div>
        {% endfor %}
    </div>

{% endblock %}